{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Mistral",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/packages/chunk-generator/src/cli/cli.ts",
      "runtimeArgs": ["--experimental-strip-types"],
      "args": [
        "--spec",
        "${workspaceFolder}/../mistral-docs/openapi.yaml",
        "-p",
        "${workspaceFolder}/../mistral-docs/docs/api",
        "-c",
        "${workspaceFolder}/../mistral-docs/src/components/speakeasy",
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Redis",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/packages/chunk-generator/src/cli/cli.ts",
      "runtimeArgs": ["--experimental-strip-types"],
      "args": [
        "--spec",
        "${workspaceFolder}/../redis-docs/content/integrate/redis-data-integration/reference/api-reference/openapi.json",
        "-p",
        "${workspaceFolder}/../docs-docusaurus-playground/docs/api",
        "-c",
        "${workspaceFolder}/../docs-docusaurus-playground/src/components/speakeasy",
      ]
    },
  ]
}