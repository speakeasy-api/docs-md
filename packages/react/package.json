{
  "name": "@speakeasy-api/docs-md-react",
  "private": false,
  "version": "0.1.27",
  "type": "module",
  "exports": {
    "./docusaurus.css": {
      "default": "./dist/docusaurus.css"
    },
    "./nextra.css": {
      "default": "./dist/nextra.css"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsc && cpx \"src/**/*.css\" dist/",
    "prettier": "prettier --config ../../prettier.config.mjs --ignore-path ../../.gitignore \"**/*.{js,jsx,mjs,ts,tsx,mts}\"",
    "format": "npm run prettier -- --write",
    "check-formatting": "npm run prettier -- --check",
    "lint": "eslint \"**/*.{js,jsx,mjs,ts,tsx,mts}\"",
    "storybook": "storybook dev -p 6006",
    "type-check": "tsc --noEmit"
  },
  "author": "",
  "license": "UNLICENSED",
  "description": "",
  "dependencies": {
    "@codesandbox/sandpack-client": "^2.19.8",
    "@codesandbox/sandpack-react": "^2.20.0",
    "@speakeasy-api/docs-md-shared": "*",
    "arg": "^5.0.2",
    "chalk": "^5.3.0",
    "change-case": "^5.4.4",
    "clsx": "^2.1.1",
    "console-feed": "^3.8.0",
    "cpx2": "^8.0.0",
    "js-yaml": "^4.1.0",
    "react-use-measure": "^2.1.7",
    "zod": "^3.25.56"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.0.1",
    "@storybook/addon-a11y": "^9.0.12",
    "@storybook/addon-docs": "^9.0.12",
    "@storybook/addon-vitest": "^9.0.12",
    "@storybook/react": "^9.0.12",
    "@storybook/react-vite": "^9.0.12",
    "@types/js-yaml": "^4.0.9",
    "@types/mdx": "^2.0.13",
    "@types/node": "^22.15.17",
    "@types/react": "^19.1.6",
    "eslint-plugin-storybook": "^9.0.12",
    "globals": "^16.3.0",
    "react": "^19.1.0",
    "storybook": "^9.0.12",
    "type-fest": "^4.41.0",
    "typescript": "^5.8.3"
  }
}